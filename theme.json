{
    "version": 2,
    "templateParts": [
      {
        "name": "header",
        "title": "Ssom-Header",
        "area": "header"
      },
      {
        "name": "footer",
        "title": "Ssom-Footer",
        "area": "footer"
      }
    ],
    "patterns": [
      "ssom/hero",
      "ssom/about", 
      "ssom/cta",
      "ssom/blog",
      "ssom/lessen",
      "ssom/reviews",
      "ssom/contact",
      "ssom-over-ons-page",
      "ssom-gitaarles-page",
      "ssom-contact-page",
      "ssom-blog-home"

    ],
    "settings": {
      "layout": {
        "contentSize": "1200px",
        "wideSize": "100%",
        "contentSizes": [
          { "slug": "small", "size": "800px", "name": "Small" },
          { "slug": "medium", "size": "1000px", "name": "Medium" },
          { "slug": "large", "size": "1200px", "name": "Large" },
          { "slug": "xl", "size": "1400px", "name": "Extra Large" },
          { "slug": "full", "size": "100%", "name": "Full Width" }
        ]
      },
      "spacing": {
        "units": ["px", "em", "rem", "%", "vw", "vh"],
        "spacingSizes": [
          { "slug": "10", "size": "10px", "name": "10" },
          { "slug": "20", "size": "20px", "name": "20" },
          { "slug": "30", "size": "30px", "name": "30" },
          { "slug": "40", "size": "40px", "name": "40" },
          { "slug": "50", "size": "50px", "name": "50" },
          { "slug": "60", "size": "60px", "name": "60" },
          { "slug": "80", "size": "80px", "name": "80" },
          { "slug": "100", "size": "100px", "name": "100" }
        ]
      },
      "color": {
        "palette": [
          { "slug": "base", "color": "#ffffff", "name": "Base" },
          { "slug": "text", "color": "#111111", "name": "Text" },
          { "slug": "accent", "color": "#b8ea3f", "name": "Accent" },
          { "slug": "accent-dark", "color": "#000000", "name": "Accent Dark" }
        ]
      },
      "typography": {
        "fontFamilies": [
          {
            "slug": "manrope",
            "name": "Manrope",
            "fontFamily": "'Manrope', sans-serif"
          }
        ],
        "fontSizes": [
          { "slug": "xxs", "size": "12px", "name": "Extra Extra Small" },
          { "slug": "small", "size": "14px", "name": "Small" },
          { "slug": "medium", "size": "18px", "name": "Medium" },
          { "slug": "large", "size": "32px", "name": "Large" },
          { "slug": "xl", "size": "48px", "name": "Extra Large" },
          { "slug": "xxl", "size": "64px", "name": "Extra Extra Large" }
        ],
        "fontWeights": [
          { "slug": "regular", "name": "Regular", "value": "400" },
          { "slug": "medium", "name": "Medium", "value": "500" },
          { "slug": "bold", "name": "Bold", "value": "700" },
          { "slug": "black", "name": "Black", "value": "900" }
        ]
      }
    },
    "styles": {
      "typography": {
        "fontFamily": "var(--wp--preset--font-family--manrope)",
        "fontSize": "var(--wp--preset--font-size--medium)"
      },
      "blocks": {
        "core/button": {
          "typography": {
            "fontFamily": "var(--wp--preset--font-family--manrope)",
            "fontWeight": "var(--wp--preset--font-weight--bold)",
            "fontSize": "var(--wp--preset--font-size--medium)"
          },
          "color": {
            "background": "var(--wp--preset--color--accent)",
            "text": "var(--wp--preset--color--accent-dark)"
          },
          "border": {
            "radius": "8px"
          },
          "spacing": {
            "padding": {
              "top": "var(--wp--preset--spacing--20)",
              "right": "var(--wp--preset--spacing--30)",
              "bottom": "var(--wp--preset--spacing--20)",
              "left": "var(--wp--preset--spacing--30)"
            }
          }
        }
      },
      "elements": {
        "h1": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontWeight": "var(--wp--preset--font-weight--bold)",
          "fontSize": "var(--wp--preset--font-size--xl)",
          "lineHeight": "1.2",
          "color": "var(--wp--preset--color--accent-dark)",
          "textAlign": "left"
        },
        "h2": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontWeight": "var(--wp--preset--font-weight--bold)",
          "fontSize": "var(--wp--preset--font-size--xl)",
          "lineHeight": "1.2",
          "color": "var(--wp--preset--color--accent-dark)"
        },
        "button": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontWeight": "var(--wp--preset--font-weight--bold)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "color": {
            "background": "var(--wp--preset--color--accent-dark)",
            "text": "var(--wp--preset--color--base)"
          },
          "border": {
            "radius": "8px"
          },
          ":hover": {
            "color": {
              "background": "var(--wp--preset--color--accent)",
              "text": "var(--wp--preset--color--base)"
            }
          }
        },
        "buttonCta": {
          "fontFamily": "var(--wp--preset--font-family--manrope)",
          "fontWeight": "var(--wp--preset--font-weight--bold)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "color": {
            "background": "var(--wp--preset--color--accent)",
            "text": "var(--wp--preset--color--accent-dark)"
          },
          "border": {
            "radius": "8px"
          },
          "spacing": {
            "padding": {
              "top": "var(--wp--preset--spacing--20)",
              "right": "var(--wp--preset--spacing--30)",
              "bottom": "var(--wp--preset--spacing--20)",
              "left": "var(--wp--preset--spacing--30)"
            }
          },
          ":hover": {
            "color": {
              "background": "var(--wp--preset--color--accent-dark)",
              "text": "var(--wp--preset--color--accent)"
            }
          }
        },
        "link": {
          "color": "var(--wp--preset--color--base)",
          ":hover": {
            "color": "var(--wp--preset--color--accent-dark)"
          }
        }
      }
    }
  }
  